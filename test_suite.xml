<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">
<suite name="AutomationTestSuite" verbose="1" parallel="false">
    <test name="LoginAndRegisterTests">
        <classes>
            <class name="pl.automaty.tests.LoginTest"/>
            <class name="pl.automaty.tests.RegisterTest"/>
        </classes>
